@model tac.Models.OverviewList

<ul class="block-grid-md-4 block-grid-sm-3 block-grid-xs-1">
    @foreach (var navigationItem in Model)
    {
        <li>
            <div class="thumbnail">
                <a href="@navigationItem.Url">@navigationItem.DecorationBanner</a>
                <div class="caption">
                    <h3 class="teaser-heading">@navigationItem.ContentHeading</h3>
                    <a href="@navigationItem.Url" class="btn btn-default">Read more</a>
                </div>
            </div>  
        </li>
    }
</ul>
